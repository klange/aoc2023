        let coord, delta = line.split(' @ ')
        let coords = [int(x) for x in coord.split(', ')]
        let deltas = [int(x) for x in delta.split(', ')]
        nodes.append((coords,deltas))
        for d in range(len(deltas)):
            if deltas[d] < md[d]:
                md[d] = deltas[d]
            if deltas[d] > Md[d]:
                Md[d] = deltas[d]
        dset.add(tuple(deltas))

    import fileio
