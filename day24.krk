        print(f's.add({node[0][1]} + t{j} * {node[1][1]} == y + dy * t{j})',file=sat)
        print(f's.add({node[0][2]} + t{j} * {node[1][2]} == z + dz * t{j})',file=sat)
        j += 1
        if j == 3:
            break

    print('print(s.check())',file=sat)
